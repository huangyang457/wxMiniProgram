<view>
  <view class="cover">
    <image src="/images/logo.jpg" class="logo" />
  </view>
  <view class="title" bind:tap="clickTitle">{{title}}</view>
  <view class="choose">
    <navigator class="join" url="/pages/form/index">创建小组</navigator>
    <navigator class="join" url="/pages/list/index">加入小组</navigator>
    <navigator class="join" url="/pages/tip/index?groupId={{groupId[0]}}">我的小组</navigator>
    <navigator class="join" url="/pages/group/index?groupId={{groupId[0]}}">其它小组</navigator>
  </view>
</view>